<h1>List of amazing NFTs</h1>

{{#if products}}
    <ul>
        {{#each products}}
            <li>
                <a href="/products/{{this.id}}">
                    {{this.title}}
                </a>
            </li>
        {{/each}}
    </ul>
{{else}}
    <p>No products meet that search criteria.</p>
{{/if}}


